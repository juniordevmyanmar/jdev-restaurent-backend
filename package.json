{
  "name": "jdev-music-player-backend",
  "version": "1.0.0",
  "main": "index.ts",
  "repository": "git@github.com:juniordevmyanmar/jdev-music-player-backend.git",
  "author": "AungAyeThan <aungayethan2015@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest --maxConcurrency=1",
    "tsc": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "dev:debug": "ts-node-dev --transpile-only --respawn --inspect=4321 --project tsconfig.json src/index.ts",
    "prod": "tsc && node bin/index.js",
    "lint": "tslint **/*.ts",
    "prettify": "prettier --config ./.prettierrc.js --write \"./*.{js,ts}\" \"./**/*.{js,ts}\"",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:generate": "npx sequelize-cli migration:generate  --name",
    "db:model:generate": "npx  sequelize-cli model:generate --name ",
    "db:seed:generate": "npx sequelize-cli seed:generate --name ",
    "db:seed": "npx sequelize-cli db:seed --seed",
    "db:seed:undo": "npx sequelize-cli db:seed:undo --seed ",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:migrate:revert:all": "npx  sequelize-cli db:migrate:undo:all",
    "db:migrate:revert:to": "npx  sequelize-cli db:migrate:undo:all --to "
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/node": "^18.15.11",
    "@types/pg": "^8.6.6",
    "bcrypt": "^5.1.0",
    "body-parser": "^1.20.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jose": "^4.13.1",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "prettier": "^2.8.7",
    "sequelize": "^6.30.0",
    "sequelize-cli": "6.6.0",
    "typescript": "^5.0.2"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}